<!DOCTYPE html>
<html lang="en">

<head>
  <%- include("../include/headLink")%>
    <!-- Title Page-->
    <title>Add Enquiry</title>
</head>

<body class="animsition">
  <div class="page-wrapper">
    <!-- HEADER MOBILE-->
    <%- include("../include/navbar") %>
      <!-- PAGE CONTAINER-->
      <div class="page-container">
        <!-- HEADER DESKTOP-->
        <%- include("../include/header")%>
          <!-- MAIN CONTENT-->
          <div class="main-content">
            <div class="section__content section__content--p30">
              <div class="container-fluid">
                <form id="enquiryForm">
                  <div class="row">
                    <div class="col-lg-6">
                      <div class="card">
                        <div class="card-header">Enquiry Info</div>
                        <div class="card-body">
                          <div class="card-title">
                            <h3 class="text-center title-2">Enquiry Info</h3> </div>
                          <hr>
                          <form action="" method="post" novalidate="novalidate">
                            <div class="form-group">
                              <label for="enquiryDate" class="control-label mb-1">Enquiry Date</label>
                              <input id="enquiryDate" name="enquiryDate" type="date" class="form-control" aria-required="true" aria-invalid="false"> </div>
                            <div class="form-group">
                              <label for="marketingPerson" class=" form-control-label">Select Marketing Person</label>
                              <select name="marketingPerson" id="marketingPerson" class="form-control">

                              </select>
                            </div>
                            <div class="form-group has-success">
                              <label for="refrenceBy" class="control-label mb-1">Refrence By</label>
                              <input type="text" id="refrenceBy" name="refrenceBy" placeholder="Enter Refrence By" class="form-control"> </div>
                            <hr>
                            <div class="card-title">
                              <h3 class="text-center title-2">Contact Info</h3> </div>
                            <hr>
                            <div class="form-group">
                              <label for="firstContactPersonName" class=" form-control-label">1st Contact Person Name</label>
                              <input type="text" id="firstContactPersonName" name="firstContactPersonName" placeholder="Enter 1st Contact Person Name" class="form-control"> </div>
                            <div class="form-group">
                              <label for="firstContactPersonMobile" class=" form-control-label">1st Person Contact No.</label>
                              <input type="number" id="firstContactPersonMobile" name="firstContactPersonMobile" placeholder="Enter 1st Person Contact No." class="form-control"> </div>
                            <div class="form-group">
                              <label for="secondContactPersonName" class=" form-control-label">2nd Contact Person Name</label>
                              <input type="text" id="secondContactPersonName" name="secondContactPersonName" placeholder="Enter 2nd Contact Person Name" class="form-control"> </div>
                            <div class="form-group">
                              <label for="secondContactPersonMobile" class=" form-control-label">2nd Person Contact No.</label>
                              <input type="number" id="secondContactPersonMobile" name="secondContactPersonMobile" placeholder="Enter 2nd Person Contact No." class="form-control"> </div>
                              <hr>
                            <div class="card-title">
                            <h3 class="text-center title-2">Enquiry Detail</h3> </div>
                          <div class="form-group">
                            <label for="shopName" class=" form-control-label">Shop Name</label>
                            <input type="text" id="shopName" name="shopName" placeholder="Enter your shop name" class="form-control"> 
                          </div>
                          <div class="form-group">
                            <label for="enquiryStatus" class=" form-control-label">Select Enquiry Status</label>
                            <select name="enquiryStatus" id="enquiryStatus" class="form-control">
                              <option value="0">Please select Enquiry Status</option>
                              <option value="New Lead">New Lead</option>
                              <option value="Lead">Lead</option>
                              <option value="Enquiry">Enquiry</option>
                              <option value="New Dealer">New Dealer</option>
                              <option value="Confirm Dealer">Confirm Dealer</option>
                              <option value="Not Confirm">Not Confirm</option>
                            </select>
                          </div>
                          <div class="form-group">
                            <label for="shopType" class=" form-control-label">Select Shop Type</label>
                            <select name="shopType" id="shopType" class="form-control">
                              <option value="0">Please select Shop Type</option>
                              <option value="Existing Dealer">Existing Dealer</option>
                              <option value="Enquiry for Dealer">Enquiry for Dealer</option>
                              <option value="Faricator">Faricator</option>
                              <option value="Wholeseller">Wholeseller</option>
                              <option value="Customer">Customer</option>
                              <option value="Builder">Builder</option>
                              <option value="Architecture">Architecture</option>
                              <option value="Contractor">Contractor</option>
                            </select>
                          </div>
                          <div class="form-group">
                            <label for="category" class=" form-control-label">Select Shop Category</label>
                            <select name="category" id="category" class="form-control">
                              <option value="0">Please select Shop Category</option>
                              <option value="VIP">VIP</option>
                              <option value="D+++">D+++</option>
                              <option value="D++">D++</option>
                              <option value="D+">D+</option>
                              <option value="D">D</option>
                              <option value="D-">D-</option>
                            </select>
                          </div>
                          </form>
                        </div>
                      </div>
                    </div>
                    <div class="col-lg-6">
                      <div class="card">
                        <div class="card-header"> <strong>Enquiry</strong> <small> Detail</small> </div>
                        <div class="card-body card-block">
                          <div class="form-group">
                            <label for="state" class=" form-control-label">GSNT No.</label>
                            <input type="text" id="state" name="gstNo" placeholder="Enter GSNT No." class="form-control"> 
                          </div>
                          <div class="form-group">
                            <label for="mobile" class=" form-control-label">Mobile</label>
                            <input type="number" name="mobile" id="mobile" placeholder="Enter Mobile No." class="form-control"> </div>
                          <div class="form-group">
                            <label for="email" class=" form-control-label">E-mail</label>
                            <input type="email" id="email" name="email" placeholder="Enter E-mail" class="form-control"> </div>
                          <div class="form-group">
                            <label for="state" class=" form-control-label">State</label>
                            <input type="text" id="state" name="state" placeholder="Enter State" class="form-control"> </div>
                          <div class="form-group">
                            <label for="city" class=" form-control-label">City</label>
                            <input type="text" id="city" name="city" placeholder="Enter City" class="form-control"> </div>
                          <div class="form-group">
                            <label for="address" class=" form-control-label">Address</label>
                            <input type="text" id="address" name="address" placeholder="Enter Address" class="form-control"> </div>
                          <div class="form-group">
                            <label for="requirement" class=" form-control-label">Requirement</label>
                            <input type="text" id="requirement" name="requirement" placeholder="Enter Requirement" class="form-control"> </div>
                          <div class="form-group">
                            <label for="currentWorking" class=" form-control-label">Current Working</label>
                            <input type="text" id="currentWorking" name="currentWorking" placeholder="Enter Current Working" class="form-control"> 
                          </div>
                          <div class="form-group">
                            <label for="currentWorking" class=" form-control-label">Transport Name</label>
                            <input type="text" id="currentWorking" name="transportName" placeholder="Enter Transport Name" class="form-control"> 
                          </div>
                          <div class="form-group">
                            <label for="currentWorking" class=" form-control-label">Transport Number</label>
                            <input type="number" id="currentWorking" name="transportNumber" placeholder="Enter Transport Number" class="form-control"> 
                          </div>
                          <hr>
                          <div class="card-title">
                              <h3 class="text-center title-2">Intrested Item</h3> 
                          </div>
                          <div class="form-check">
                              <div class="checkbox">
                                  <label for="checkbox1" class="form-check-label ">
                                      <input type="checkbox" id="checkbox1" name="interestedItem1" value="1" class="form-check-input">PVC Sheets 4mm - 5mm 
                                  </label>
                              </div>
                              <div class="checkbox">
                                  <label for="checkbox1" class="form-check-label ">
                                      <input type="checkbox" id="checkbox1" name="interestedItem2" value="1" class="form-check-input">PVC Sheets 6mm to 18mm
                                  </label>
                              </div>
                              <div class="checkbox">
                                  <label for="checkbox1" class="form-check-label ">
                                      <input type="checkbox" id="checkbox1" name="interestedItem3" value="1" class="form-check-input">PVC Doors
                                  </label>
                              </div>
                              <div class="checkbox">
                                  <label for="checkbox1" class="form-check-label ">
                                      <input type="checkbox" id="checkbox1" name="interestedItem4" value="1" class="form-check-input">WPC Doors & Choukhats
                                  </label>
                              </div>
                              <div class="checkbox">
                                  <label for="checkbox1" class="form-check-label ">
                                      <input type="checkbox" id="checkbox1" name="interestedItem5" value="1" class="form-check-input">Hollow Items
                                  </label>
                              </div>
                              <div class="checkbox">
                                  <label for="checkbox1" class="form-check-label ">
                                      <input type="checkbox" id="checkbox1" name="interestedItem6" value="1" class="form-check-input">Hollow Doors
                                  </label>
                              </div>
                              <div class="checkbox">
                                  <label for="checkbox1" class="form-check-label ">
                                      <input type="checkbox" id="checkbox1" name="interestedItem7" value="1" class="form-check-input">Kitchen
                                  </label>
                              </div>
                              <div class="checkbox">
                                  <label for="checkbox1" class="form-check-label ">
                                      <input type="checkbox" id="checkbox1" name="interestedItem8" value="1" class="form-check-input">Wash Unit
                                  </label>
                              </div>
                              <br>
                          </div>
                              <div class="form-group">
                                <input type="text" id="currentWorking" name="interestedItem9" placeholder="Enter Other Interested Item" class="form-control">
                              </div>
                            <hr>
                        </div>
                      </div>
                    </div>
                  </div>
                  <button type="submit" class="btn btn-lg btn-info btn-block submitData"> <span id="payment-button-amount">Submit</span> </button>
                </form>
              </div>
            </div>
          </div>
      </div>
      <!-- Jquery JS-->
      <%- include("../include/jsLink") %>
        <script>
        $(document).ready(function() {
          //Submit Enquiry Data
          $(document).on('click', '.submitData', function() {
            event.preventDefault();
            console.log("2");
            $.ajax({
              url: '/create-enquiry',
              method: 'POST',
              data: $('#enquiryForm').serialize(),
              success: function(responseSubmit) {
                if(responseSubmit.msg == 'success') {
                  alert('Enquiry Added Successfully');
                }
                if(responseSubmit.msg == 'fail') {
                  alert('Enquiry Already Exist!!');
                }
              }
            });
          });
          var selectOption = "";

          function marketingPerson(data, index) {
            selectOption += '<option value="' + data._id + '">' + data.name + '</option>'
          }
          enquiryList();

          function enquiryList() {
            var modelData = "";
            $.ajax({
              url: '/all-user',
              method: 'get',
              dataType: 'json',
              success: function(response) {
                console.log(response);
                if(response.msg == 'success') {
                  response.user.forEach((data, index) => {
                    marketingPerson(data, index);
                  });
                  $('#marketingPerson').html(selectOption);
                }
              }
            })
          }
        });
        </script>
</body>

</html>
<!-- end document-->