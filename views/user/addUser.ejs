<!DOCTYPE html>
<html lang="en">

<head>
  <%- include("../include/headLink")%>
    <!-- Title Page-->
    <title>Add User</title>
</head>

<body class="animsition">
  <div class="page-wrapper">
    <!-- HEADER MOBILE-->
    <%- include("../include/navbar") %>
      <!-- PAGE CONTAINER-->
      <div class="page-container">
        <!-- HEADER DESKTOP-->
        <%- include("../include/header")%>
          <!-- MAIN CONTENT-->
          <div class="main-content">
            <div class="section__content section__content--p30">
              <div class="container-fluid">
                <form action="/create-user" method="post">
                  <div class="row">
                    <div class="col-lg-6">
                      <div class="card">
                        <div class="card-header">User Info</div>
                        <div class="card-body">
                          <div class="card-title">
                            <h3 class="text-center title-2">User Info</h3> </div>
                          <hr>
                          
                            <div class="form-group">
                              <label for="enquiryDate" class="control-label mb-1">Joining Date</label>
                              <input id="enquiryDate" name="joiningDate" type="date" class="form-control" aria-required="true" aria-invalid="false"> 
                            </div>
                            <div class="form-group has-success">
                              <label for="refrenceBy" class="control-label mb-1">Name</label>
                              <input type="text" id="refrenceBy" name="name" placeholder="Enter Name" class="form-control"> 
                            </div>
                            <div class="form-group has-success">
                              <label for="refrenceBy" class="control-label mb-1">Email</label>
                              <input type="email" id="refrenceBy" name="email" placeholder="Enter Email" class="form-control"> 
                            </div>
                            <div class="form-group has-success">
                              <label for="refrenceBy" class="control-label mb-1">Father/Husband Name</label>
                              <input type="text" id="refrenceBy" name="FHName" placeholder="FHName" class="form-control"> 
                            </div>
                            <div class="form-group">
                              <label for="enquiryDate" class="control-label mb-1">Date of Birth</label>
                              <input id="enquiryDate" name="dob" type="date" class="form-control" aria-required="true" aria-invalid="false"> 
                            </div>
                            <div class="form-group">
                              <label for="firstContactPersonMobile" class=" form-control-label">Contact No.</label>
                              <input type="number" id="firstContactPersonMobile" name="contactNo" placeholder="Enter Contact No." class="form-control"> 
                            </div>
                            <div class="form-group">
                              <label for="firstContactPersonMobile" class=" form-control-label">Whatsapp Contact No.</label>
                              <input type="number" id="firstContactPersonMobile" name="whatsappContactNo" placeholder="Enter Whatsapp Contact No." class="form-control"> 
                            </div>
                            <div class="form-group has-success">
                              <label for="refrenceBy" class="control-label mb-1">Local Address</label>
                              <input type="text" id="refrenceBy" name="localAddress" placeholder="Enter Local Address" class="form-control"> 
                            </div>
                            <div class="form-group has-success">
                              <label for="refrenceBy" class="control-label mb-1">Permanent Address</label>
                              <input type="text" id="refrenceBy" name="permanentAddress" placeholder="Enter Permanent Address" class="form-control"> 
                            </div>
                            <hr>
                            <div class="form-group has-success">
                              <label for="refrenceBy" class="control-label mb-1">Designation</label>
                              <input type="text" id="refrenceBy" name="designation" placeholder="Enter Designation" class="form-control"> 
                            </div>
                            <div class="form-group has-success">
                              <label for="refrenceBy" class="control-label mb-1">Work Status</label>
                              <input type="text" id="refrenceBy" name="workStatus" placeholder="Enter Work Status" class="form-control"> 
                            </div>
                            <div class="form-group has-success">
                              <label for="refrenceBy" class="control-label mb-1">Salary</label>
                              <input type="number" id="refrenceBy" name="salary" placeholder="Enter Salary" class="form-control"> 
                            </div>
                            <div class="form-group has-success">
                              <label for="refrenceBy" class="control-label mb-1">PL</label>
                              <select class="form-control" name="pl">
                                <option value="0">0</option>
                                <option value="1">1</option>
                                <option value="1.5">1.5</option>
                              </select>
                            </div>
                            <div class="form-group has-success">
                              <label for="refrenceBy" class="control-label mb-1">Blood Group</label>
                              <select class="form-control" name="bloodGroup">
                                <option value="A+">A+</option>
                                <option value="A-">A-</option>
                                <option value="B+">B+</option>
                                <option value="B-">B-</option>
                                <option value="o+">o+</option>
                                <option value="o-">o-</option>
                                <option value="AB+">AB+</option>
                                <option value="AB-">AB-</option>
                              </select>
                            </div>
                            <div class="form-group has-success">
                              <label for="refrenceBy" class="control-label mb-1">Adhar Number</label>
                              <input type="number" id="refrenceBy" name="adharNo" placeholder="Enter Adhar Number" class="form-control"> 
                            </div>
                            <div class="form-group has-success">
                              <label for="refrenceBy" class="control-label mb-1">Votor Card No</label>
                              <input type="text" id="refrenceBy" name="votorNo" placeholder="Enter Votor Card No" class="form-control"> 
                            </div>
                            <div class="form-group has-success">
                              <label for="refrenceBy" class="control-label mb-1">Pan Card No</label>
                              <input type="text" id="refrenceBy" name="panNo" placeholder="Enter Pan Card No" class="form-control"> 
                            </div>
                            <div class="form-group has-success">
                              <label for="refrenceBy" class="control-label mb-1">Driving License No</label>
                              <input type="text" id="refrenceBy" name="drivingLicenseNo" placeholder="Enter Driving License No" class="form-control"> 
                            </div>
                            <div class="form-group has-success">
                              <label for="refrenceBy" class="control-label mb-1">PF Account No</label>
                              <input type="text" id="refrenceBy" name="pfNo" placeholder="Enter PF Account No" class="form-control"> 
                            </div>
                            <div class="form-group has-success">
                              <label for="refrenceBy" class="control-label mb-1">ESI Insurance No</label>
                              <input type="text" id="refrenceBy" name="esiInsuranceNo" placeholder="Enter ESI Insurance No" class="form-control"> 
                            </div>
                            <div class="form-group has-success">
                              <label for="refrenceBy" class="control-label mb-1">ESI UAN No</label>
                              <input type="text" id="refrenceBy" name="esiUanNo" placeholder="Enter ESI UAN No" class="form-control"> 
                            </div>
                            <div class="form-group has-success">
                              <label for="refrenceBy" class="control-label mb-1">ESI Member Id</label>
                              <input type="text" id="refrenceBy" name="esiMemberId" placeholder="Enter ESI Member Id" class="form-control"> 
                            </div>

                        </div>
                      </div>
                    </div>
                    <div class="col-lg-6">
                      <div class="card">
                        <div class="card-header">Bank Detail </div>
                        <div class="card-body card-block">
                          <div class="card-title">
                            <h3 class="text-center title-2">Bank Detail</h3> 
                          </div>
                          <div class="form-group has-success">
                              <label for="refrenceBy" class="control-label mb-1">Bank Name</label>
                              <input type="text" id="refrenceBy" name="bankName" placeholder="Enter Bank Name" class="form-control"> 
                          </div>
                          <div class="form-group has-success">
                              <label for="refrenceBy" class="control-label mb-1">Bank Address</label>
                              <input type="text" id="refrenceBy" name="bankAddress" placeholder="Enter Bank Address" class="form-control"> 
                          </div>           
                          <div class="form-group has-success">
                              <label for="refrenceBy" class="control-label mb-1">Bank Account No</label>
                              <input type="number" id="refrenceBy" name="bankAccountNo" placeholder="Enter Account No." class="form-control"> 
                          </div>               
                          <div class="form-group has-success">
                              <label for="refrenceBy" class="control-label mb-1">IFSC Code</label>
                              <input type="text" id="refrenceBy" name="bankIfscCode" placeholder="Enter Bank IFSC Code" class="form-control"> 
                          </div>
                          <hr>
                          <div class="card-title">
                            <h3 class="text-center title-2">Additional Inforamtion</h3> 
                          </div>
                          <hr>
                          <div class="form-group has-success">
                              <label for="refrenceBy" class="control-label mb-1">Name</label>
                              <input type="text" id="refrenceBy" name="reName1" placeholder="Enter Name" class="form-control"> 
                          </div>
                          <div class="form-group has-success">
                              <label for="refrenceBy" class="control-label mb-1">Relation to Employee</label>
                              <input type="text" id="refrenceBy" name="re1" placeholder="Enter Relation to Employee" class="form-control"> 
                          </div>
                          <div class="form-group has-success">
                              <label for="refrenceBy" class="control-label mb-1">Mobile No.</label>
                              <input type="number" id="refrenceBy" name="reMobile1" placeholder="Enter Mobile No." class="form-control"> 
                          </div>
                          <div class="form-group has-success">
                              <label for="refrenceBy" class="control-label mb-1">Name</label>
                              <input type="text" id="refrenceBy" name="reName2" placeholder="Enter Name" class="form-control"> 
                          </div>
                          <div class="form-group has-success">
                              <label for="refrenceBy" class="control-label mb-1">Relation to Employee</label>
                              <input type="text" id="refrenceBy" name="re2" placeholder="Enter Relation to Employee" class="form-control"> 
                          </div>
                          <div class="form-group has-success">
                              <label for="refrenceBy" class="control-label mb-1">Mobile No.</label>
                              <input type="number" id="refrenceBy" name="reMobile2" placeholder="Enter Mobile No." class="form-control"> 
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                  <button type="submit" class="btn btn-lg btn-info btn-block submitData"> <span id="payment-button-amount">Submit</span> </button>
                </form>
              </div>
            </div>
          </div>
      </div>
      <!-- Jquery JS-->
      <%- include("../include/jsLink") %>
        <script>
        $(document).ready(function() {
        });
        </script>
</body>

</html>
<!-- end document-->